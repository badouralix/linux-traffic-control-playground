{
	"build": {
		"dockerfile": "Dockerfile"
	},
	"capAdd": [
		"NET_ADMIN"
	],
	"containerEnv": {
		"JUPYTER_PATH": "${containerWorkspaceFolder}"
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-toolsai.jupyter"
			]
		}
	},
	"postCreateCommand": "poetry install"
}
